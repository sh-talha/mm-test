<style>
.psc_footer .container {
  max-width: 1535px;
  padding: 0 20px;
  margin: 0 auto;
}
.psc_footer .footer_inner {
  display: flex;
  flex-direction: column;
  gap: 65px;
}
.psc_footer .footer_content{
  display: flex;
  flex-direction: column;
  gap: 13px;
  max-width: 1092px;
  margin: 0 auto;
}
.psc_footer h3{
  text-align: center;
      font-family: "Town 50 Chic";
  font-size: 16.921px;
  font-style: normal;
  font-weight: 400;
  line-height: 25.6px;
  letter-spacing: 3.584px;
  text-transform: uppercase;
  margin: 0;
}
.psc_footer h2{
  text-align: center;
      font-family: "Town 50 Chic";
  font-size: 47px;
  font-style: normal;
  font-weight: 400;
  line-height: 57.6px;
  letter-spacing: 0.512px;
  text-transform: uppercase;
  margin: 0;
}
.psc_footer .field {
  margin: 0 auto;
  max-width: 801px;
  gap: 10px;
}
.psc_footer .field__input{
  background: transparent;
  border: 1px solid #FFF;
  max-width: 482px;
  height: unset;
  padding: 17px 53px;
  color: #fff;
     font-family: "Town 50 Chic";
}
input::placeholder {
  display:block !important;
  opacity:1 !important;
  visibility:visible !important;
}
.field__input:focus{
  padding: 17px 53px !important;
}
.psc_footer .field:after{
  bottom:unset;
  left:unset;
  border: unset;
}
.psc_footer .field__button {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 19.8px;
  font-style: normal;
  font-weight: 400;
  line-height: 32px;
  letter-spacing: 4.992px;
  text-transform: uppercase;
  width: 236px;
  background: #FFAD14;
  position: unset;
  color: #FFF;
  height: 100%;
      font-family: "Town 50 Chic";
}
.psc_footer .btn_svg {
  display: flex;
  gap: 5px;
  align-items: center;
}
.psc_footer .submit_svg {
  background: #FFAD14;
  height: 70px;
  width: 73px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.psc_footer .field__label{
      font-family: "Town 50 Chic";
  font-size: 19.8px;
  font-style: normal;
  font-weight: 400;
  line-height: normal;
  letter-spacing: 4.966px;
  text-transform: uppercase;
  left: 8%;
  top: 50%;
  transform: translateY(-50%);
}
.psc_footer .footer_link {
  display: grid;
  align-items: center;
  justify-content: center;
  grid-template-columns: 37% 63%;
}
.psc_footer .bottom_nav,
.psc_footer .ft_social_links {
  display: flex;
  align-items: center;
  justify-content: space-around;
  border-top: 2px solid #FFF;
  border-right: 1px solid #FFF;
  border-bottom: 2px solid #FFF;
  border-left: 2px solid #FFF;
  margin: 0;
}
.psc_footer .bottom_nav{
  height: 100%;
}
.psc_footer .ft_social_links {
  padding: 35px 0;
}
.psc_footer li{
  list-style:none;
}
.psc_footer a{
  font-family: 'Inter';
  font-size: 15px;
  font-style: normal;
  font-weight: 800;
  line-height: 33.28px;
  letter-spacing: 3.328px;
  text-transform: uppercase;
  text-decoration: none;
  display: flex;
}
#psc_footer-{{ section.id }} {
  background-color:{{section.settings.bg_color}};
  padding-top:{{ section.settings.padding_top_desktop }}px;
  padding-bottom:{{ section.settings.padding_bottom_desktop }}px;
  margin-top:{{ section.settings.margin_top_desktop }}px;
  margin-bottom:{{ section.settings.margin_bottom_desktop }}px;
}
#psc_footer-{{ section.id }} h2,
#psc_footer-{{ section.id }} a,
#psc_footer-{{ section.id }} .field__label,
#psc_footer-{{ section.id }} input::placeholder{
  color: {{section.settings.text_color}};
}
#psc_footer-{{ section.id }} h3{
  color: {{section.settings.top_text_color}};
}
  .footer_botm{
    padding: 40px 0px;
    background: #fff;
  }
  .footer_botm p{
    margin: 0;
    font-family: "Town 50 Chic";
    font-size: 15.822px;
    font-style: normal;
    font-weight: 400;
    line-height: 29.44px;
    letter-spacing: 3.328px;
    text-transform: uppercase;
    color: #000;
    width: 50%;
    text-align: center;
  }
  .footer_botm .footer_botm-inner{
    display: flex;
    width: 100%;
    justify-content: space-between;
  }
  .footer_botm-link{
    width: 50%;
    display: flex;
    justify-content: flex-end;
  } 
  .footer_botm a{
    font-family: "Town 50 Chic";
    font-size: 16.211px;
    font-style: normal;
    font-weight: 400;
    line-height: 29.44px;
    letter-spacing: 3.328px;
    text-transform: uppercase;
    color: #000;
    text-decoration: none;
    padding: 0px 100px;
    position: relative;
    border-right: 4px solid;
  }
  .footer_botm a:last-child{
     border-right: 0px;
  }
@media (max-width: 1199px){
  
  .psc_footer h2 {
    font-size: 37px;
    line-height: 45px;
  }
}
@media (max-width: 991px){
  .psc_footer .footer_inner {
    gap: 45px;
  }
  .psc_footer h2 {
    font-size: 30px;
    line-height: 35px;
  }
  .footer_botm .footer_botm-inner {
    width: 100%;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
}
  .footer_botm p {
    letter-spacing: 3.328px;
    width: 100%;
}
}
@media (max-width: 767px){
  #psc_footer-{{ section.id }} {
    padding-top:{{ section.settings.padding_top_mobile }}px;
    padding-bottom:{{ section.settings.padding_bottom_mobile }}px;
    margin-top:{{ section.settings.margin_top_mobile }}px;
    margin-bottom:{{ section.settings.margin_bottom_mobile }}px;
  }
  .psc_footer .field {
    flex-direction: column;
    align-items: center;
  }
  .psc_footer .field__button {
    width: 100%;
    font-size: 14px;
    height: 45px;
  }
  .psc_footer .submit_svg {
    height: 45px;
    width: 45px;
  }
  .psc_footer h3 {
    font-size: 12px;
  }
  .psc_footer h2 {
    font-size: 20px;
    line-height: 28px;
  }
  .psc_footer .footer_inner {
    gap: 35px;
  }
  .psc_footer .footer_link {
    grid-template-columns: 1fr;
    gap: 20px;
  }
  .psc_footer .ft_social_links {
    padding: 15px 0;
  }
  .psc_footer .bottom_nav {
    padding: 10px 0;
    margin-bottom:20px;
    flex-wrap: wrap;
  }
  .psc_footer .bottom_nav li {
    width: 100%;
}
  .psc_footer .bottom_nav a{
    justify-content: center;
  }
  .psc_footer .btn_svg{
    width: 100%;
    max-width: 420px;
  }
  .footer_botm p {
    font-size: 14.822px;
    line-height: 20.44px;
}
  .footer_botm a {
    font-size: 14.211px;
    line-height: 18.44px;
    padding: 10px 50px;
}
  .footer_botm {
    padding: 20px 0px;
}
  .footer_botm-link {
    display: flex;
    align-items: center;
    padding-top: 10px;
    justify-content: center;
}
  .psc_footer .submit_svg svg{
        width: 12px;
  }
}
</style>
<section class="psc_footer" id="psc_footer-{{ section.id }}">
  <div class="container">
    <div class="footer_inner">
      <div class="footer_content">
        {% if section.settings.subhead != blank %}
          <h3>{{ section.settings.subhead }}</h3>
        {% endif %}
        {% if section.settings.headline != blank %}
          <h2>{{ section.settings.headline }}</h2>
        {% endif %}
      </div>
        <div class="newsletter-form__field-wrapper">
          <div class="field">
            <input
              id="NewsletterForm--{{ section.id }}"
              type="email"
              name="contact[email]"
              class="field__input"
              value="{{ form.email }}"
              aria-required="true"
              autocorrect="off"
              autocapitalize="off"
              autocomplete="email"
              {% if form.errors %}
                autofocus
                aria-invalid="true"
                aria-describedby="ContactFooter-error"
              {% elsif form.posted_successfully? %}
                aria-describedby="ContactFooter-success"
              {% endif %}
              placeholder="Enter Email"
              required
            >

            <span class="btn_svg">
              <button
                type="submit"
                class="newsletter-form__button field__button"
                name="commit"
                id="Subscribe"
                aria-label="{{ 'newsletter.button_label' | t }}"
              >
                Submit
              </button>
              <span class="submit_svg">
                <svg xmlns="http://www.w3.org/2000/svg" width="19" height="36" viewBox="0 0 19 36" fill="none">
  <g clip-path="url(#clip0_4_19)">
    <path d="M1.57806 1.3905L17.7026 17.5151L1.57806 33.6396" stroke="white" stroke-width="1.97726" stroke-linecap="round" stroke-linejoin="round"/>
  </g>
  <defs>
    <clipPath id="clip0_4_19">
      <rect width="18.5111" height="35.045" fill="white" transform="translate(0.179443 -0.00756836)"/>
    </clipPath>
  </defs>
</svg>
              </span>
            </span>
          </div>
          {%- if form.errors -%}
            <small class="newsletter-form__message form__message" id="ContactFooter-error">
              {%- render 'icon-error' -%}
              {{- form.errors.translated_fields.email | capitalize }}
              {{ form.errors.messages.email -}}
            </small>
          {%- endif -%}
        </div>
      <div class=footer_link>
