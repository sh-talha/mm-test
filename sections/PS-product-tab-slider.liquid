{% comment %} <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.5.8/slick.min.css"/>
<link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.5.8/slick-theme.min.css"/> {% endcomment %}
<style>

.product_tab_slider {
  width: 100%;
  height: auto;
  overflow: hidden;
  margin-bottom:60px;
}
.product_tab_slider .contianer {
  max-width: 100%;
  padding: 0;
}
.product_tab_slider .tab-menu {
  display: flex; 
  width: 100%;
  padding: 10px 0;
  justify-content: center;
  align-items: flex-start;
  background: #EFEFEF;
}
.product_tab_slider .tab_btn {
  display: flex;
  justify-content: center;
  gap: 70px;
  align-items: center;
  padding: 0;
  margin: 0px;
}
.product_tab_slider .tab_btn li {
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding: 0 20px;
}
.product_tab_slider .tab_btn .tab_link_btn {
  color: #ADAFB2;
  text-align: center;
  font-family: Arial;
  font-size: 18px;
  font-weight: 700; 
  line-height: 26px;
  letter-spacing: 3.584px;
  text-transform: uppercase;
}
.product_tab_slider .tab_btn li .active_span {
  color: #000;
} 
.product_tab_slider .slick-next:before, .product_tab_slider .slick-prev:before{
  filter: invert(1);
}
.product_tab_slider .tab {
  visibility:hidden;
  opacity:0;
  height:0;
  overflow:hidden;
  margin: 0;
}
.product_tab_slider .active_tab {
  visibility:visible;
  opacity:1;
  height:auto;
}
.product_tab_slider img {
  width: 100%;
  height: 100%;
  object-fit: contain;
}
.product_tab_slider h2 {
  text-align: center;
  font-family: Righteous;
  font-size: 149.219px;
  font-style: normal;
  line-height: 113.92px;
  letter-spacing: -1.536px;
  text-transform: uppercase;
  padding-bottom: 20px;
  margin: 46px 0px 0px;
  font-weight: 700;
}
.product_tab_slider .tab_content_col {
  padding: 0 20px;
}
.product_tab_slider .slider_inner {
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 70px;
  max-width: 90%;
  margin: 0 auto;
}
.product_tab_slider .image {
  max-width: 230px;
  height: 500px;
  margin: 0 auto; 
}
.product_tab_slider .slick-prev, 
.product_tab_slider .slick-next {
  top: 60%;
}
@media screen and (max-width: 1520px){
.product_tab_slider h2 {
  font-size: 110px;
  line-height: 100px;
}
.product_tab_slider .image {
  max-width: 200px;
  height: auto;
}
}
@media screen and (max-width: 1199px){
.product_tab_slider .slick-prev, 
.product_tab_slider .slick-next {
  top: 50%;
  z-index: 9;
}
.product_tab_slider .tab_btn {
  gap: 35px;
}
.product_tab_slider h2 {
  font-size: 90px;
  margin-top: 20px;
}
.product_tab_slider .slider_inner {
  gap: 35px;
}
.product_tab_slider .image {
  max-width: 180px;
}
}
@media screen and (max-width: 991px){
.product_tab_slider .slick-prev, .product_tab_slider .slick-next {
  top: 50%;
  z-index: 9;
}
.product_tab_slider .tab_btn {
  gap: 20px;
}
.product_tab_slider .tab_btn .tab_link_btn {
  font-size: 14px;
  line-height: 20px;
  letter-spacing: 2px;
}
.product_tab_slider h2 {
  font-size: 70px;
  line-height: 78px;
}
.product_tab_slider .image {
  max-width: 160px;
}
}
@media screen and (max-width: 767px) {
.product_tab_slider .slick-prev,
.product_tab_slider .slick-next {
  top: 40%;
  z-index: 9;
}
.product_tab_slider .tab_btn {
  flex-direction: column;
  gap: 10px;
}
.product_tab_slider h2 {
  font-size: 35px;
  line-height: 40px;
  padding-bottom: 10px;
}
.product_tab_slider .slider_inner {
  gap: 15px;
  flex-wrap: wrap;
}
.product_tab_slider .image {
  max-width: 110px;
}
}
</style>
