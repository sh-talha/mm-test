<script>
  document.addEventListener("DOMContentLoaded", (event) => {
    $('.tab_link_btn').click(function () {
        $(".tab").removeClass('active_tab');
        $(".tab[data-id='" + $(this).attr('data-id') + "']").addClass("active_tab");
        $(".tab_link_btn").removeClass('active_span');
        $(this).addClass('active_span');
        var tabIndex = $(".tab_link_btn").index(this);
    });  
    
    $(".slider_inner").slick({ 
      slidesToShow: 6,
      slidesToScroll: 1,
      autoplaySpeed: 0,
      autoplay: false, 
      variableWidth: false, 
      arrows: true,
      responsive: [
          {
            breakpoint: 1199,
            settings: {
              slidesToShow: 5
            }
          },
          {
            breakpoint: 991,
            settings: {
              slidesToShow: 4
            }
          },
          {
            breakpoint: 520,
            settings: {
              slidesToShow: 3
            }
          }
        ]
    }); 
});
</script>

<script>
  document.addEventListener("DOMContentLoaded", function() {
    var images = document.querySelectorAll(".product_tab_slider .image");
    images.forEach(function(image) {
      image.addEventListener("click", function(event) {
        event.preventDefault();
        let url = image.getAttribute('href');
        console.log(url);
        fetch(url)
        .then(res => res.text())
        .then(data => {
           let div = document.createElement('div');
            div.innerHTML = data;
            //console.log(div.querySelector('.main-pdp').innerHTML);
            let new_data = div.querySelector('.main-pdp').innerHTML;
            document.querySelector('.main-pdp').innerHTML = new_data
            
          })
      });
    });

  });
</script>




